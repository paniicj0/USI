<Window x:Class="LangLang.View.TutorGradingExamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LangLang.View"
        mc:Ignorable="d"
        Loaded="TutorGradingExamWindow_Loaded"
        Title="TutorGradingExamWindow" Height="800" Width="1300">
    <Grid>
        <DataGrid x:Name="dgvStudents" Height="200" Margin="0,30,0,0" HorizontalAlignment="Center" SelectedCellsChanged="dgvStudents_SelectedCellsChanged"/>
        <Button x:Name="btnGrade" Content="Grade this student" Margin="410,600,380,0" FontSize="18" Click="btnGrade_Click" Height="50" VerticalAlignment="Top"/>
    </Grid>
</Window>
