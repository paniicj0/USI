<Window x:Class="LangLang.View.CoursesCompletedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LangLang.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Loaded="CoursesCompletedWindow_Load"
        Title="CompletedCoursesWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgCompletedCourses" Height="200" Margin="0,20,0,0" HorizontalAlignment="Center"/>
        <Label Content="Completed courses: " HorizontalAlignment="Center" Margin="0,60,0,0" VerticalAlignment="Top" FontSize="20" Height="35"/>
        <Button x:Name="btnGradeTutor" Content="Grade tutor" HorizontalAlignment="Center" Margin="0,370,0,0" VerticalAlignment="Top" FontSize="20" Click="btnGradeTutor_Click"/>
    </Grid>
</Window>
